        <script>
            function loadCategoryProducts(obj){

                var products_type = $(obj).data('products_type');
                var category_id = $(obj).data('category_id');
                var swiper_name = $(obj).data('swiper_name');
                var container = $(obj).data('products_container_id');
                var selected_tab_title = $(obj).html();

                var swiperTitleObj = $(obj).closest('.courses-swiper-container').find('.swiper-title');
                var newTabTitle = '';
                if(category_id!=''){
                    newTabTitle = swiperTitleObj.data('tab-title-prefix')+selected_tab_title;
                }else{
                    newTabTitle = swiperTitleObj.data('tab-title-for-all-items');
                }
                swiperTitleObj.html(newTabTitle);

                if ($('#'+container).html().trim() !== "") {
                    return;
                }

                form_data = {
                    '_token': "MQAF0A5wpt7wLDq3d98zJVmZfcJz3zf3tEzvI8jC",
                    'category_id': category_id,
                    'products_type': products_type,
                    'swiper_name': swiper_name
                }

                loadingSwiperItems(container);

                $.ajax({
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    },
                    type: 'POST',
                    url: "https://faradars.org/category/products/ajax",
                    'data': form_data,
                    success: function (response) {
                        if (response.status) {
                            $('#'+container).html(response.data.view);

                            loadMainPageProductSwipers(swiper_name);

                        } else {
                            toastr.warning("خطا");
                        }
                    }
                })

            }
            function loading(status) {
                if (status) {
                    $('body').addClass('is-loading');
                }else{
                    $('body').removeClass('is-loading');
                }
            }

            function loadingSwiperItems(container) {
                var product_item_loading_content = $('#loading-product-item #loading-product-one-item').html();
                var product_item_load_more_button = $('#loading-product-item #loading-product-load-more-button').html();

                var loading_content = '';
                                    loading_content = product_item_loading_content + product_item_loading_content + product_item_loading_content + product_item_loading_content + product_item_load_more_button;
                
                $('#' + container).html(loading_content);
            }

        </script>
    
	<div class="backToUp" onclick="backToUp()" >
		<img src="https://faradars.org/wp-content/uploads/2020/11/23/to_top.png" >
    </div>

    <script>

        $('#course-cover .cover-play-icon .cover-vjs-big-play-button').click(function(){
            $('#course-cover-image').hide();
            $('#course-cover .cover-play-icon').hide();
            $('#course-cover .videoSummary').show();
            $('#course-cover div[name="video"]').css("padding-top", "0");
            $(".videoSummary .vjs-big-play-button").trigger("click");
        });


        
        
        
        
        
        
        

         function seenNotifications(userId) {
             $.ajax({
                 type: 'post',
                 url: 'https://faradars.org/my-account/notifications/seen'
             });
         }
	</script>
	<script>
		document.getElementById("q").onclick = function () {
			myFunction()
		};

		function myFunction() {
			document.getElementById("myDropdown").classList.toggle("searchShow");
		}

		window.onclick = function (event) {

			if (!event.target.matches('#q')) {

				var dropdowns = document.getElementsByClassName("searchDropdown-content");

				for (let i = 0; i < dropdowns.length; i++) {
					var openDropdown = dropdowns[i];
					if (openDropdown.classList.contains('searchShow')) {
						openDropdown.classList.remove('searchShow');
					}
				}
			}
		}
		var previousScroll = 0;
        
        $(window).scroll(function(event){
            var scroll = $(this).scrollTop();
            if (scroll > previousScroll){
                $(".backToUp").hide()
            } else {
                $(".backToUp").show()
            }
            previousScroll = scroll;
        });
        
